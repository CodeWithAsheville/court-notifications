    "copy-to-clipboard": "^3.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "i18next": "^21.1.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-backend": "^1.3.1",
    "i18next-http-middleware": "^3.1.4",
    "jsdom": "^16.4.0",
    "knex": "^0.95.8",
    "mustache": "^4.2.0",
    "pg": "^8.7.1",
    "react-i18next": "^11.12.0",
    "rollbar": "^2.24.0",
    "tough-cookie": "^4.0.0",
    "twilio": "^3.67.1",
